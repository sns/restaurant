  },
  "homepage": "https://github.com/sns/restaurant#readme",
  "devDependencies": {},
  "dependencies": {
    "@types/express": "^4.16.1",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.4.1",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "nodemon": "^1.18.10"
  }
}
