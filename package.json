{
  "name": "restaurant",
  "version": "1.0.0",
  "description": "Full Stack Restaurant App Powered by Express and React",
  "main": "server.js",
  "scripts": {
    "start": "node ./build/app.js",
    "dev-client": "cd client && npm start",
    "dev-server": "nodemon --exec ./node_modules/.bin/ts-node -- ./server/server.ts",
